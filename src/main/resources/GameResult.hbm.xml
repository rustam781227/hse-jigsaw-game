<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.db.GameResult" table="GAME_RESULT" schema="APP">
        <id name="id">
            <column name="ID" sql-type="integer"/>
            <generator class="identity"> </generator>
        </id>
        <property name="date">
            <column name="DATE" sql-type="char(100)" length="100"/>
        </property>
        <property name="time">
            <column name="TIME" sql-type="char(50)" length="50"/>
        </property>
        <property name="score">
            <column name="COUNT" sql-type="integer"/>
        </property>
        <property name="login">
            <column name="LOGIN" sql-type="char(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>